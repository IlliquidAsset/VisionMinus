<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3.5dip" />
            <gradient android:startColor="#fff0f0f0" android:endColor="#fff0f0f0" android:angle="0.0" android:centerY="0.75" android:centerColor="#fff0f0f0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="3.5dip" />
                <gradient android:startColor="#ff3a75f2" android:endColor="#ff3a75f2" android:angle="0.0" android:centerY="0.75" android:centerColor="#ff3a75f2" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3.5dip" />
                <gradient android:startColor="#ff3a75f2" android:endColor="#ff3a75f2" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>
