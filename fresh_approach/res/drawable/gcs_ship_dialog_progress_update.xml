<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/gcs_ship_update_progress_bg_start" android:endColor="@color/gcs_ship_update_progress_bg_end" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="left" android:clipOrientation="horizontal">
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/gcs_ship_update_progress_start" android:endColor="@color/gcs_ship_update_progress_end" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>
