<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3.0dip" />
            <gradient android:startColor="#ffedeff2" android:endColor="#ffedeff2" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffedeff2" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3.0dip" />
                <gradient android:startColor="#ff399afb" android:endColor="#ff399afb" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>
